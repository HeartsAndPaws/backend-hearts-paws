{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "dotenv -e .env.development -- node --max-old-space-size=512 dist/main",
    "start:dev": "node --max-old-space-size=512 ./node_modules/dotenv-cli -e .env.development -- nest start --watch",
    "start:staging": "dotenv -e .env.staging -- nest start",
    "start:prod": "dotenv -e .env.production -- node dist/main",
    "generate": "prisma generate --schema=src/prisma/schema.prisma",
    "prisma:dev": "dotenv -e .env.development -- npx prisma migrate dev --schema=src/prisma/schema.prisma",
    "prisma:staging": "dotenv -e .env.staging -- npx prisma db push --schema=src/prisma/schema.prisma",
    "prisma:prod": "dotenv -e .env.production -- npx prisma db push --schema=src/prisma/schema.prisma",
    "prisma:studio": "dotenv -e .env.development -- npx prisma studio --schema=src/prisma/schema.prisma",
    "prisma:reset": "dotenv -e .env.development -- npx prisma migrate reset --schema=src/prisma/schema.prisma",
    "prisma:resetstaging": "dotenv -e .env.staging -- npx prisma migrate reset --schema=src/prisma/schema.prisma",
    "prisma:resetprod": "dotenv -e .env.production -- npx prisma migrate reset --schema=src/prisma/schema.prisma",
    "seed": "dotenv -e .env.development -- ts-node src/prisma/seed.ts",
    "seed:staging": "dotenv -e .env.staging -- ts-node src/prisma/seed.ts",
    "seed:prod": "dotenv -e .env.production -- ts-node src/prisma/seed.ts",
    "start:debug": "nest start --debug --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/platform-socket.io": "^11.1.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/websockets": "^11.1.3",
    "@prisma/client": "^6.9.0",
    "@supabase/supabase-js": "^2.50.0",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^2.6.1",
    "cookie-parser": "^1.4.7",
    "jwks-rsa": "^3.2.0",
    "multer": "^2.0.1",
    "nodemailer": "^7.0.3",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.1",
    "pug": "^3.0.3",
    "reflect-metadata": "^0.2.2",
    "resend": "^4.6.0",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "stripe": "^18.2.1",
    "swagger-ui-express": "^5.0.1",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.13",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/stripe": "^8.0.417",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.9.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
